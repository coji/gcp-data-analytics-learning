# 変数定義ファイルのサンプル
# 実際に使用する際は、このファイルを variables.tf にリネームして使用してください

variable "project_id" {
  description = "GCPプロジェクトID"
  type        = string
  default     = "YOUR_PROJECT_ID"
}

variable "location" {
  description = "BigQueryのロケーション"
  type        = string
  default     = "asia-northeast1"
}

variable "dataset_id" {
  description = "データセットID"
  type        = string
  default     = "terraform_learning_test"
}

variable "environment" {
  description = "環境名（dev, staging, prod）"
  type        = string
  default     = "learning"
}
